{{#extends "layout/main.handlebars"}}

{{#content "title"}}Dashboard{{/content}}

{{#content "body"}}
{{#each posts as |post|}}
    <h1>Dashboard</h1>
    <ul>
        {{#each post}}
            <li>{{this.title}}</li>
        {{/each}}
    </ul>
    <a href="/dashboard/create">Create New Post</a>
{{/content}}